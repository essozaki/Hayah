@model AreasVM
@{
    ViewData["Title"] = "Index";
}
<!-- Scroll - horizontal and vertical table -->
<section id="horizontal-vertical">
    <div class="dt-buttons btn-group">
       
    </div>
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title"> </h4>
                </div>
                <div class="card-content">
                    <div class="card-body card-dashboard">
                        <div class="table-responsive">
                            <form asp-controller="Areas" asp-action="Delete" class="form-group ">

                                <table class="table nowrap scroll-horizontal-vertical">
                                    <thead>
                                        <tr>
                                            <th> حذف المنطقه </th>
                                        </tr>



                                    </thead>
                                    <tbody>

                                        <tr>
                                            <td>المنطقه </td>
                                            <td>@Model.Area_Name </td>
                                        </tr>

                                     
                                        <tr>
                                            <td>المدينه </td>
                                            <td>@Model.Cities.Cite_Name </td>
                                        </tr>

                                     


                                    </tbody>

                                </table>

                                <center>
                                    <input type="submit" value="حذف" class="btn btn-danger" />
                                    <a asp-controller="Areas" asp-action="Index" class="btn btn-info">العودة</a>
                                </center>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--/ Scroll - horizontal and vertical table -->
