@model CitiesVM;

@{
    ViewData["Title"] = "Create";
}


<!-- Basic Vertical form layout section start -->
<section id="basic-vertical-layouts">
    <div class="row match-height">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">اضافه مدينه جديد</h4>
            </div>
            <div class="card-content">
                <div class="card-body">
                    <form class="form form-vertical" asp-action="Create" asp-controller="Cities" enctype="multipart/form-data">
                        <div class="form-body">
                            <div class="row">



                                <div class="col-12">
                                    <div class="form-group">
                                        <label for="first-name-vertical"> اسم المدينه</label>
                                        <input type="text" id="first-name-vertical" class="form-control" asp-for="Cite_Name" placeholder="اسم المدينه ">
                                    </div>
                                </div>

                                <div class="col-12">
                                    <button type="submit" class="btn btn-primary mr-1 mb-1">حفظ</button>
                                    <button type="reset" class="btn btn-outline-warning mr-1 mb-1">الغاء</button>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
</section>


